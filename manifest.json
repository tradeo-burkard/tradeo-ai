{
  "manifest_version": 3,
  "name": "Tradeo Support AI (Gemini + Plenty)",
  "version": "3.2",
  "description": "FreeScout AI Integration mit Plentymarkets Anbindung.",
  "permissions": ["storage"],
  "host_permissions": [
    "https://desk.tradeo.de/*",
    "https://p7843.my.plentysystems.com/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": ["https://desk.tradeo.de/*"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ]
}