{
  "manifest_version": 3,
  "name": "Tradeo Support AI (gpt-oss-120b + Plenty)",
  "version": "3.2",
  "description": "FreeScout AI Integration mit Plentymarkets Anbindung.",
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "https://desk.tradeo.de/*",
    "https://p7843.my.plentysystems.com/*",
    "https://api.runpod.ai/*"
  ],
  "background": {
    "service_worker": "background.js"
  },
  "content_scripts": [
    {
      "matches": [
        "https://desk.tradeo.de/*"
      ],
      "js": [
        "systemPrompts.js",
        "content.js"
      ],
      "css": [
        "styles.css"
      ]
    }
  ]
}
