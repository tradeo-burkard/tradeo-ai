{
  "manifest_version": 3,
  "name": "Tradeo Support AI (Gemini 3.0 Pro)",
  "version": "3.0",
  "description": "Liest FreeScout Tickets und generiert Antworten mit Gemini 3.0 Pro.",
  "permissions": ["storage"],
  "host_permissions": ["https://desk.tradeo.de/*"],
  "content_scripts": [
    {
      "matches": ["https://desk.tradeo.de/*"],
      "js": ["content.js"],
      "css": ["styles.css"]
    }
  ]
}